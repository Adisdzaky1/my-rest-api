{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "routes/api.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/home/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/login/(.*)",
      "dest": "routes/users.js"
    },
    {
      "src": "/docs",
      "dest": "/index.js"
    }
  ]
}
